{
    "name": "gerosa2015", 
    "reference": "http://www.sciencedirect.com/science/article/pii/S2405471215001465",
    "description": "Dataset from Gerosa et al 2015 for E.coli (batch growth).",
    "organism": "Escherichia coli",
    "strain": "Escherichia coli K-12 BW25113",
    
    "conditions": ["Acetate", "Fructose", "Galactose", "Glucose", "Glycerol", "Gluconate", "Pyruvate", "Succinate"],

    "carbon_source": ["ac", "fru", "gal", "glc", "glyc", "glcn", "pyr", "succ"],

    "aerobic": [true, true, true, true, true, true, true, true],
    
    "gene_deletions": [null, null, null, null, null, null, null, null],

    "growth_rate": [0.2900, 0.4924, 0.1800, 0.6500, 0.4900, 0.5900, 0.3900, 0.5100],
    
    "fluxomics": {
    	"datafile": "fluxomics.csv",
    	"units": "mmol/gDW/h"
    },

    "transcriptomics": {
    	"datafile": "transcriptomics.csv",
    	"units": "a.u."
    }
    
}